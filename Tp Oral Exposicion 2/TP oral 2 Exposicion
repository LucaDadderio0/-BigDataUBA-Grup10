   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# === FUNCIONES ===\n",
    "\n",
    "def cargar_eph(path_csv):\n",
    "    \"\"\"Carga archivo de EPH individual con codificación latina y separador ';'\"\"\"\n",
    "    return pd.read_csv(path_csv, sep=';', encoding='latin1')\n",
    "\n",
    "def procesar_ingresos(df, año):\n",
    "    \"\"\"Filtra ingresos válidos y agrega columna de año\"\"\"\n",
    "    df_filtrado = df[df['ITF'] > 0].copy()\n",
    "    df_filtrado['año'] = año\n",
    "    return df_filtrado[['ITF', 'CH04', 'año']]  # CH04: sexo\n",
    "\n",
    "# === CARGA DE DATOS ===\n",
    "\n",
    "df_2018 = procesar_ingresos(cargar_eph('usu_individual_T318.csv'), 2018)\n",
    "df_2021 = procesar_ingresos(cargar_eph('usu_individual_T321.csv'), 2021)\n",
    "df_2024 = procesar_ingresos(cargar_eph('usu_individual_T324.csv'), 2024)\n",
    "\n",
    "# === UNIÓN DE DATAFRAMES ===\n",
    "\n",
    "df_total = pd.concat([df_2018, df_2021, df_2024])\n",
    "\n",
    "# === ANÁLISIS BÁSICO ===\n",
    "\n",
    "print(df_total.groupby('año')['ITF'].describe())\n",
    "\n",
    "# === GRÁFICA: DISTRIBUCIÓN DE INGRESOS ===\n",
    "\n",
    "plt.figure(figsize=(12, 6))\n",
    "sns.kdeplot(data=df_total, x='ITF', hue='año', fill=True, common_norm=False)\n",
    "plt.xlim(0, df_total['ITF'].quantile(0.99))  # recorte del 1% superior\n",
    "plt.title('Distribución de Ingresos Totales Familiares (ITF) - 3er Trimestre')\n",
    "plt.xlabel('Ingreso Total Familiar (ITF)')\n",
    "plt.ylabel('Densidad')\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
